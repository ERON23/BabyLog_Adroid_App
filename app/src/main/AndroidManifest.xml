<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maceo.babylog">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/babylog_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignUpActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BabyInfoActivity" />
        <activity
            android:name=".HomeActivity"
            android:parentActivityName=".BabyInfoActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".BabyInfoActivity" />
        </activity>
        <activity
            android:name=".FeedingActivity"
            android:label="Feeding"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity" />
        </activity>
        <activity
            android:name=".SleepActivity"
            android:label="Sleep"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity" />
        </activity>
        <activity
            android:name=".ChartActivity"
            android:label="Charts"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity" />
        </activity>
        <activity
            android:name=".DiaperChangeActivity"
            android:label="Diaper Change"
            android:parentActivityName=".HomeActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".HomeActivity" />
        </activity>
        <activity
            android:name=".NotificationActivity"
            android:label="Notifications" />
        <activity android:name=".TummyTimeActivity" />
        <activity android:name=".WeighyTrackingActivity" />
        <activity
            android:name=".FeedingBottleActivity"
            android:label="Bottle"
            android:parentActivityName=".FeedingActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FeedingActivity" />
        </activity>
        <activity
            android:name=".BreastFeedingActivity"
            android:label="Breast Feeding"
            android:parentActivityName=".FeedingActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FeedingActivity" />
        </activity>
        <activity
            android:name=".NutritionActivity"
            android:label="Meal"
            android:parentActivityName=".FeedingActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".FeedingActivity" />
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".MemoriesActivity"></activity>
    </application>

</manifest>